<?xml version="1.0"?>

<rdf:RDF 
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:dcterms="http://purl.org/dc/terms/"
	xmlns:sioc="http://rdfs.org/sioc/spec/"
	xmlns:myNameSpace="http://www.example.com/myprimerrdf#"
	xml:base="http://www.example.com/myprimerrdf"
	xmlsn:="http://www.example.com/myprimerrdf#">

	....//Creación de la clase Tweet
	<rdfs:Class rdf:about="#Tweet"/>
	
	....//Creación de la clase User
	<rdfs:Class rdf:about="#User"/>
	
	....//Creación de propiedad content
	<rdf:Property rdf:about="#content"/>
	....//Creación de propiedad creator
	<rdf:Property rdf:about="#creator"/>
	....//Creación de propiedad userAccount
	<rdf:Property rdf:about="#userAccount"/>
	
	...//Creación del individuo tweet11 con propiedades
	<Tweet rdf:about = "http://www.example.com/myprimerrdf#tweet111">
		<content>"Un tweet"^^xsd:string</content>
		<creator>"#user211"</creator>
	</Tweet>
	
	...Creación del individuo user201 con propiedades
	<User rdf:about = "http://www.example.com/myprimerrdf#user201">
		<userAccount>"@Pepe"^^xsd:string</userAccount>
	</User>
	
</rdf:RDF>